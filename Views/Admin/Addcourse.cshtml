@model School_Management_System_Practicum.Models.Subject
@{
    ViewData["Title"] = "Addcourse";
    Layout = "~/Views/Shared/Adminlayout.cshtml";
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
}



<h2>Add Subject</h2>


<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Addcourse">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="SName" class="control-label"></label>
                <input asp-for="SName" class="form-control" />
                <span asp-validation-for="SName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Showsubject">Back to List</a>
</div>


