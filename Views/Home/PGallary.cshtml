@model IEnumerable<School_Management_System_Practicum.Models.Image>
@{
    ViewData["Title"] = "PGallary";
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
    <link href="CustomStyles.css" rel="stylesheet" />
          }

    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <center> <h1>Photo Gallary</h1></center>

            </div>


            @foreach (var item in Model)
            {
                <div class="col-lg-3 col-md-4 col-sm-6">
                   
                    <a href="~/@item.pic" class="thumbnail">
                        <p>@item.ImageTitle</p>
                        <img src="~/@item.pic" />
                      
                    </a>
                </div>

            }
        </div>
    </div>




